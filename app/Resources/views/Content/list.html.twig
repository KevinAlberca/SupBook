{% extends "::base.html.twig" %}
{% block title %}Liste du contenu{% endblock %}

{% block body %}
    {#{% include "Content/add.html.twig" %}#}
    {{ include('Content/add.html.twig', {'form': form}) }}
    <ul>
    {% for c in contents %}
        <li>
            {{ c.id }}
            {{ c.content }}
            {{ c.idAuthor }}
            {{ c.postDate|date("d F Y H:i:s") }}
        </li>
    {% endfor %}
    </ul>
{% endblock %}