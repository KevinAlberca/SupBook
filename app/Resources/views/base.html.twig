<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
        <title>{% block title %}Welcome!{% endblock %}</title>
        <link href="http://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
        <link rel="stylesheet" type="text/css" href="{{ asset("css/materialize.min.css") }}" />
        <link rel="stylesheet" type="text/css" href="{{ asset("css/style.css") }}" />
        {% block stylesheets %}{% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        <div class="navbar-fixed">
            <nav>
                <div class="nav-wrapper">
                    <a href="#!" class="brand-logo">Logo</a>
                    <ul class="right hide-on-med-and-down">
                        <li><a href="#"><i class="material-icons center">search</i></a></li>
                        <li><a class="dropdown-button" href="#!" data-activates="user_dropdown"><i class="material-icons center">person_outline</i></a></li>
                    </ul>
                    <ul id="user_dropdown" class="dropdown-content">
                        <li><a href="/profile">Profile</a></li>
                        <li><a href="#!">two</a></li>
                        <li class="divider"></li>
                        <li><a href="/logout">Log out</a></li>
                    </ul>
                </div>
            </nav>
        </div>
        <div class="container">
            {% block body %}{% endblock %}
        </div>

        <script type="text/javascript" src="{{ asset("js/jquery.min.js") }}"></script>
        <script type="text/javascript" src="{{ asset("js/materialize.min.js") }}"></script>
        <script type="text/javascript" src="{{ asset("js/supbook.js") }}"></script>
        {% block javascripts %}{% endblock %}
    </body>
</html>
